SP = 67836.43
RJ = 36678.66
MG = 29229.88
ES = 27165.48
Outros = 19849.53

total = SP + RJ + MG + ES + Outros

pSP = (SP/total) * 100
pRJ = (RJ/total) * 100
pMG = (MG/total) * 100
pES = (ES/total) * 100
pOutros = (Outros/total) * 100

print('SP: {:.2f}%'.format(pSP))
print('RJ: {:.2f}%'.format(pRJ))
print('MG: {:.2f}%'.format(pMG))
print('ES: {:.2f}%'.format(pES))
print('Outros: {:.2f}%'.format(pOutros))
